;----------------------------------------------------------------------
; CorePCIF[_AHB]
;----------------------------------------------------------------------

; Override global mappings.
; Redeclare native device support for CorePCIF[_AHB] v3.x to ensure that
; global mappings don't apply because ProASIC3L/IGLOO+ to ProASIC3 mappings
; do not work for these cores. Redeclare SmartFusion to ProASIC3 mapping so
; that this does take effect.

VLNV Actel:DirectCore:COREPCIF:3.*
    include G3:ProASIC3
    include G3:ProASIC3E
    include G3:IGLOO
    include G3:IGLOOe
    include G3:Fusion
    include *:PA
    include *:Axcelerator
    include *:54SXA
    map G3:SmartFusion => G3:ProASIC3

VLNV Actel:DirectCore:COREPCIF_AHB:3.*
    include G3:ProASIC3
    include G3:ProASIC3E
    include G3:IGLOO
    include G3:IGLOOe
    include G3:Fusion
    map G3:SmartFusion => G3:ProASIC3

; Override global mappings.
; CorePCIF[_AHB] v3.5.x adds explicit support for ProASIC3L so override v3.*
; rules above while also ensuring that the global ProASIC3L to ProASIC3
; mapping does not take effect. Redeclare SmartFusion to ProASIC3 mapping so
; that this does take effect.

VLNV Actel:DirectCore:COREPCIF:3.5.*
    include G3:ProASIC3
    include G3:ProASIC3E
    include G3:ProASIC3L
    include G3:IGLOO
    include G3:IGLOOe
    include G3:Fusion
    include *:PA
    include *:Axcelerator
    include *:54SXA
    map G3:SmartFusion => G3:ProASIC3

VLNV Actel:DirectCore:COREPCIF_AHB:3.5.*
    include G3:ProASIC3
    include G3:ProASIC3E
    include G3:ProASIC3L
    include G3:IGLOO
    include G3:IGLOOe
    include G3:Fusion
    map G3:SmartFusion => G3:ProASIC3

; Override global mappings.
; CorePCIF[_AHB] v3.6.x explicitly supports all G3 except IGLOO+ and 
; SmartFusion. Override v3.x rules above while also ensuring that global
; IGLOO+ to ProASIC3 mapping does not take effect and the SmartFusion to
; ProASIC3 mapping does.

VLNV Actel:DirectCore:COREPCIF:3.6.*
    include G3:ProASIC3
    include G3:ProASIC3E
    include G3:ProASIC3L
    include G3:IGLOO
    include G3:IGLOOe
    include G3:Fusion
    include *:PA
    include *:Axcelerator
    include *:54SXA
    map G3:SmartFusion => G3:ProASIC3

VLNV Actel:DirectCore:COREPCIF_AHB:3.6.*
    include G3:ProASIC3
    include G3:ProASIC3E
    include G3:ProASIC3L
    include G3:IGLOO
    include G3:IGLOOe
    include G3:Fusion
    map G3:SmartFusion => G3:ProASIC3

; Override global mappings.
; CorePCIF_AHB v3.7.x explicitly supports all G3 (including SmartFusion) but
; not IGLOO+ so override v3.x rules above while also ensuring that global
; IGLOO+ to ProASIC3 mapping does not take effect

VLNV Actel:DirectCore:COREPCIF_AHB:3.7.*
    include G3:ProASIC3
    include G3:ProASIC3E
    include G3:ProASIC3L
    include G3:IGLOO
    include G3:IGLOOe
    include G3:Fusion
    include G3:SmartFusion

; Override rules for CorePCIF 4.0.x 
; (1) Core is packaged with min Libero version (MLV) = 11.0 but supports some 
; Libero IDE (9.0+) device families to restate MLV as 9.0 so that the core can
; be used on these device families.
; (2) Restate packaged device support to ensure that global mapping for IGLOO+
; does not take effect.
; (3) Map IGLOO2 to SmartFusion2

VLNV Actel:DirectCore:COREPCIF:4.0.*
    min_libero_version 9.0
    include PA4:SmartFusion2
    include G3:SmartFusion
    include G3:ProASIC3
    include G3:ProASIC3E
    include G3:ProASIC3L
    include G3:IGLOO
    include G3:IGLOOe
    include G3:Fusion
    include *:PA
    include *:Axcelerator
    include *:54SXA
    map PA4:IGLOO2     => PA4:SmartFusion2

; Override rules for CorePCIF_AHB 4.0.x 
; (1) Restate packaged device support to ensure that global mapping for IGLOO+
; does not take effect.
VLNV Actel:DirectCore:COREPCIF_AHB:4.0.*
    include PA4:SmartFusion2
    include PA4:IGLOO2
    include G3:SmartFusion
    include G3:ProASIC3
    include G3:ProASIC3E
    include G3:ProASIC3L
    include G3:IGLOO
    include G3:IGLOOe
    include G3:Fusion

