;----------------------------------------------------------------------
; CoreFROM
;----------------------------------------------------------------------

; CoreFROM effectively depends on G3 NVM. It's not useful with 
; SmartFusion MSS NVM. Device family support and selected mappings 
; restated here to prevent global SmartFusion => ProASIC3 mapping taking 
; effect.

VLNV Actel:DirectCore:CoreFROM:1.0
    ; Explicitly supports these device families
    include G3:ProASIC3
    include G3:ProASIC3E
    include G3:Fusion
    ; Map these device families to ProASIC3
    map G3:ProASIC3L => G3:ProASIC3
    map G3:IGLOO     => G3:ProASIC3
    map G3:IGLOOe    => G3:ProASIC3
    map G3:IGLOO+    => G3:ProASIC3

VLNV Actel:DirectCore:CoreFROM:1.1.101
    ; Linux support
    os pc linux
    ; Explicitly supports these device families
    include G3:ProASIC3
    include G3:ProASIC3E
    include G3:IGLOO
    include G3:IGLOOe
    include G3:Fusion
    ; Map these device families to ProASIC3
    map G3:ProASIC3L => G3:ProASIC3
    map G3:IGLOO+    => G3:ProASIC3

