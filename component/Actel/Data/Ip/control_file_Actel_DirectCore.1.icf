;----------------------------------------------------------------------
; CoreSDR[_AHB]
;----------------------------------------------------------------------

; Override global mappings.
; Redeclare device family support specified in the SPIRIT descriptions
; for CoreSDR[_AHB] to provide additional mappings and to ensure that
; G3 IGLOO devices (as opposed to G4 IGLOO2) are NOT supported even if
; support is declared in the CCZ/CPZ. This is because G3 IGLOO devices
; are too slow to support the SDR cores. Also map SmartFusion, SmartFusion2
; and IGLOO2 to ProASIC3 where relevant.

VLNV Actel:DirectCore:CORESDR:3.0.115
    include G3:ProASIC3
    include G3:ProASIC3E
    include G3:Fusion
    include *:Axcelerator
    map G3:ProASIC3L     => G3:ProASIC3
    map G3:SmartFusion   => G3:ProASIC3
    map PA4:SmartFusion2 => G3:ProASIC3
    map PA4:IGLOO2       => G3:ProASIC3

VLNV Actel:DirectCore:CORESDR:4.0.115
    include G3:ProASIC3
    include G3:ProASIC3E
    include G3:ProASIC3L
    include G3:Fusion
    include *:Axcelerator
    map G3:SmartFusion   => G3:ProASIC3
    map PA4:SmartFusion2 => G3:ProASIC3
    map PA4:IGLOO2       => G3:ProASIC3

VLNV Actel:DirectCore:CORESDR_AHB:4.0.142
    include G3:ProASIC3
    include G3:ProASIC3E
    include G3:ProASIC3L
    include G3:Fusion
    map G3:SmartFusion   => G3:ProASIC3

VLNV Actel:DirectCore:CORESDR_AHB:4.1.102
    include G3:ProASIC3
    include G3:ProASIC3E
    include G3:ProASIC3L
    include G3:Fusion
    include G3:SmartFusion

VLNV Actel:DirectCore:CORESDR_AHB:4.3.100
