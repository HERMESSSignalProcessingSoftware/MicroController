    <actel-cc:script>Enable('CWL', Eq('MEMORY', 'DDR3'));\
Enable('RZQ', Eq('MEMORY', 'DDR3'));\
Enable('AL', Neq('MEMORY', 'LPDDR'));\
Enable('WR', Neq('MEMORY', 'LPDDR'));\
Validate(DynamicText('Burst length of 16 can not be used for DDR3/2 memories'), Or(Eq('MEMORY', 'LPDDR'), And(Eq('MEMORY', 'DDR2'), Neq('BL_BITS', '16')), And(Eq('MEMORY', 'DDR3'), Neq('BL_BITS', '16'))));\
Validate(DynamicText('Valid CAS Latency values for DDR3 are: 5,6,7,8,9,10'), Or(And(Eq('MEMORY', 'DDR3'), GtOrEq('CL', '5')), Neq('MEMORY', 'DDR3')));\
Validate(DynamicText('Valid CAS Latency values for DDR2 are: 3,4,5,6'), Or(And(Eq('MEMORY', 'DDR2'), GtOrEq('CL', '3'), Less('CL', '7')), Neq('MEMORY', 'DDR2')));\
Validate(DynamicText('Valid CAS Latency values for LPDDR are: 2,3,4'), Or(And(Eq('MEMORY', 'LPDDR'), GtOrEq('CL', '2'), Less('CL', '5')), Neq('MEMORY', 'LPDDR')));\
Validate(DynamicText('Valid Additive CAS Latency values for DDR3 are: 0, CL-1, CL-2'), Or(And(Eq('MEMORY', 'DDR3'), Less('AL', '3')), Neq('MEMORY', 'DDR3')));\
</actel-cc:script></spirit:vendorExtensions>
