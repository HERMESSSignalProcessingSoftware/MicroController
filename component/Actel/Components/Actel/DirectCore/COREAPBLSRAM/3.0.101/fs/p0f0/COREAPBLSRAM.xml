    </actel-cc:variantParameters>
    <actel-cc:script>Enable('USRAM_NUM_LOCATIONS_DWIDTH', Eq('SEL_SRAM_TYPE', 'USRAM'));\
Tooltip('USRAM_NUM_LOCATIONS_DWIDTH', 'For APB Data Width = 32 the valid range for USRAM Depth is 64 to 2304 in steps of 64. 
For APB Data Width = 16 the valid range for USRAM Depth is 64 to 4608 in steps of 64. 
For APB Data Width = 8 the valid range for USRAM Depth is 128 to 9216 in steps of 128. ');\
Enable('LSRAM_NUM_LOCATIONS_DWIDTH', Eq('SEL_SRAM_TYPE', 'LSRAM'));\
Tooltip('LSRAM_NUM_LOCATIONS_DWIDTH', 'For APB Data Width = 32 the valid range for LSRAM Depth is 512 to 35328 in steps of 512. 
For APB Data Width = 16 the valid range for LSRAM Depth is 1024 to 70656 in steps of 1024. 
For APB Data Width = 8 the valid range for LSRAM Depth is 2048 to 141312 in steps of 2048. ');\
Validate('
	LSRAM:
	For APB Data Width = 32 the valid range for LSRAM Depth is 512 to 35328 in steps of 512. 
	For APB Data Width = 16 the valid range for LSRAM Depth is 1024 to 70656 in steps of 1024. 
	For APB Data Width =  8 the valid range for LSRAM Depth is 2048 to 141312 in steps of 2048. 

	USRAM:
	For APB Data Width = 32 the valid range for USRAM Depth is 64 to 2304 in steps of 64. 
	For APB Data Width = 16 the valid range for USRAM Depth is 64 to 4608 in steps of 64, 
	For APB Data Width =  8 the valid range for USRAM Depth is 128 to 9216 in steps of 128. 
	
	Refer to the handbook for more information', Or(Or(And(Eq('SEL_SRAM_TYPE', 'LSRAM'), Eq('APB_DWIDTH', '32'), And(GtOrEq('LSRAM_NUM_LOCATIONS_DWIDTH', '512'), Less('LSRAM_NUM_LOCATIONS_DWIDTH', '35329')), Not(Mod('LSRAM_NUM_LOCATIONS_DWIDTH', '512'))), And(Eq('SEL_SRAM_TYPE', 'LSRAM'), Eq('APB_DWIDTH', '16'), And(GtOrEq('LSRAM_NUM_LOCATIONS_DWIDTH', '1024'), Less('LSRAM_NUM_LOCATIONS_DWIDTH', '70657')), Not(Mod('LSRAM_NUM_LOCATIONS_DWIDTH', '1024'))), And(Eq('SEL_SRAM_TYPE', 'LSRAM'), Eq('APB_DWIDTH', '8'), And(GtOrEq('LSRAM_NUM_LOCATIONS_DWIDTH', '2048'), Less('LSRAM_NUM_LOCATIONS_DWIDTH', '141313')), Not(Mod('LSRAM_NUM_LOCATIONS_DWIDTH', '2048')))), Or(And(Eq('SEL_SRAM_TYPE', 'USRAM'), Eq('APB_DWIDTH', '32'), And(GtOrEq('USRAM_NUM_LOCATIONS_DWIDTH', '64'), Less('USRAM_NUM_LOCATIONS_DWIDTH', '2305')), Not(Mod('USRAM_NUM_LOCATIONS_DWIDTH', '64'))), And(Eq('SEL_SRAM_TYPE', 'USRAM'), Eq('APB_DWIDTH', '16'), And(GtOrEq('USRAM_NUM_LOCATIONS_DWIDTH', '64'), Less('USRAM_NUM_LOCATIONS_DWIDTH', '4609')), Not(Mod('USRAM_NUM_LOCATIONS_DWIDTH', '64'))), And(Eq('SEL_SRAM_TYPE', 'USRAM'), Eq('APB_DWIDTH', '8'), And(GtOrEq('USRAM_NUM_LOCATIONS_DWIDTH', '128'), Less('USRAM_NUM_LOCATIONS_DWIDTH', '9217')), Not(Mod('USRAM_NUM_LOCATIONS_DWIDTH', '128'))))));\
</actel-cc:script></spirit:vendorExtensions>
