    <spirit:signals>
      <spirit:signal>
        <spirit:name>CLK</spirit:name>
        <spirit:direction>in</spirit:direction>
        <spirit:vendorExtensions>
          <actel-cc:requiredConnection></actel-cc:requiredConnection>
          <actel-cc:description>Encoder clock signal.</actel-cc:description></spirit:vendorExtensions>
      </spirit:signal>
      <spirit:signal>
        <spirit:name>CLKEN</spirit:name>
        <spirit:direction>in</spirit:direction>
        <spirit:vendorExtensions>
          <actel-cc:requiredConnection></actel-cc:requiredConnection>
          <actel-cc:description>Encoder clock enable signal.</actel-cc:description></spirit:vendorExtensions>
      </spirit:signal>
      <spirit:signal>
        <spirit:name>CODEOUTP</spirit:name>
        <spirit:direction>out</spirit:direction>
        <spirit:left spirit:dependency="id('MM') - 1" spirit:resolve="dependent">4</spirit:left>
        <spirit:right>0</spirit:right>
        <spirit:vendorExtensions>
          <actel-cc:requiredConnection></actel-cc:requiredConnection>
          <actel-cc:description>Encoded data (codeword) output. The output is m-bit wide.</actel-cc:description></spirit:vendorExtensions>
      </spirit:signal>
      <spirit:signal>
        <spirit:name>DATAINP</spirit:name>
        <spirit:direction>in</spirit:direction>
        <spirit:left spirit:dependency="id('MM') - 1" spirit:resolve="dependent">4</spirit:left>
        <spirit:right>0</spirit:right>
        <spirit:vendorExtensions>
          <actel-cc:requiredConnection></actel-cc:requiredConnection>
          <actel-cc:description>Data input to be encoded. The input bus is m-bit wide.</actel-cc:description></spirit:vendorExtensions>
      </spirit:signal>
      <spirit:signal>
        <spirit:name>NGRST</spirit:name>
        <spirit:direction>in</spirit:direction>
        <spirit:vendorExtensions>
          <actel-cc:requiredConnection></actel-cc:requiredConnection>
          <actel-cc:description>Asynchronous reset. Active low.</actel-cc:description></spirit:vendorExtensions>
      </spirit:signal>
      <spirit:signal>
        <spirit:name>RDY</spirit:name>
        <spirit:direction>out</spirit:direction>
        <spirit:vendorExtensions>
          <actel-cc:requiredConnection></actel-cc:requiredConnection>
          <actel-cc:description>(Output RS Code) Ready. It signals valid codeword is present at the core output.</actel-cc:description></spirit:vendorExtensions>
      </spirit:signal>
      <spirit:signal>
        <spirit:name>RFD</spirit:name>
        <spirit:direction>out</spirit:direction>
        <spirit:vendorExtensions>
          <actel-cc:description>Ready For Data. The signal is active when the encoder is ready to accept a new data portion of k symbols on the dataInp.</actel-cc:description></spirit:vendorExtensions>
      </spirit:signal>
      <spirit:signal>
        <spirit:name>RFS</spirit:name>
        <spirit:direction>out</spirit:direction>
        <spirit:vendorExtensions>
          <actel-cc:requiredConnection></actel-cc:requiredConnection>
          <actel-cc:description>Ready For Start. The signal is active when the encoder is ready to accept a new start signal.</actel-cc:description></spirit:vendorExtensions>
      </spirit:signal>
      <spirit:signal>
        <spirit:name>RST</spirit:name>
        <spirit:direction>in</spirit:direction>
        <spirit:vendorExtensions>
          <actel-cc:description>Synchronous reset.</actel-cc:description></spirit:vendorExtensions>
      </spirit:signal>
      <spirit:signal>
        <spirit:name>START</spirit:name>
        <spirit:direction>in</spirit:direction>
        <spirit:vendorExtensions>
          <actel-cc:requiredConnection></actel-cc:requiredConnection>
          <actel-cc:description>This signal starts a new codeword cycle. It informs the encoder that at the next clock interval the first m-bit data symbol of a k-symbol sequence will appear at the dataInp.</actel-cc:description></spirit:vendorExtensions>
      </spirit:signal>
    </spirit:signals>
    <spirit:hwParameters>
      <spirit:hwParameter spirit:id="FAMILY" spirit:name="FAMILY" spirit:prompt="FPGA Family:" spirit:dataType="int" spirit:choiceRef="Choice_FamilyValue" spirit:choiceStyle="combo" spirit:configGroups="globalConfig" spirit:resolve="user">11</spirit:hwParameter>
      <spirit:hwParameter spirit:id="MM" spirit:name="MM" spirit:prompt="[m] Symbol width (bits):" spirit:dataType="int" spirit:choiceRef="Choice_3to8" spirit:resolve="user">3</spirit:hwParameter>
      <spirit:hwParameter spirit:id="TT" spirit:name="TT" spirit:prompt="[t] Number of correctable errors:" spirit:dataType="int" spirit:resolve="user" spirit:minimum="1" spirit:maximum="16">1</spirit:hwParameter>
      <spirit:hwParameter spirit:id="NN" spirit:name="NN" spirit:prompt="[n] Codeword length (symbols):" spirit:dataType="int" spirit:resolve="user" spirit:minimum="5" spirit:maximum="255">5</spirit:hwParameter>
      <spirit:hwParameter spirit:id="CCSDS" spirit:name="CCSDS" spirit:prompt="CCSDS Compatibility" spirit:dataType="int" spirit:choiceRef="Choice_CCSDS" spirit:resolve="user">0</spirit:hwParameter>
      <spirit:hwParameter spirit:id="CCSDS_CONV" spirit:name="CCSDS_CONV" spirit:prompt="Dual to conventional basis input converter" spirit:dataType="boolean" spirit:resolve="user">0</spirit:hwParameter>
    </spirit:hwParameters>
    <spirit:deletedhwParameters>
    </spirit:deletedhwParameters>
  <spirit:vendorExtensions>
    <actel-cc:variantParameters>
      <actel-cc:variantParameter spirit:id="HDL_license" spirit:name="HDL_license" spirit:prompt="License:" spirit:dataType="string" actel-cc:choiceRef="Choice_HDL_License" spirit:resolve="user">U</actel-cc:variantParameter>
      <actel-cc:variantParameter spirit:id="testbench" spirit:name="testbench" spirit:prompt="Testbench:" spirit:dataType="int" actel-cc:choiceRef="Choice_testbench" spirit:resolve="user">1</actel-cc:variantParameter>
      <actel-cc:variantParameter spirit:id="Prim_poly" spirit:name="Prim_poly" spirit:prompt="Primitive polynomial" spirit:dataType="int" actel-cc:choiceRef="Choice_PP" spirit:resolve="user">11</actel-cc:variantParameter>
      <actel-cc:variantParameter spirit:id="first_root" spirit:name="first_root" spirit:prompt="First Root" actel-cc:choiceRef="" spirit:dataType="int" spirit:resolve="user" spirit:minimum="0" spirit:maximum="255">0</actel-cc:variantParameter>
    </actel-cc:variantParameters>
    <actel-cc:script>Validate('Illegal Primitie Polynomial for this value of [m]', Or(And(InRange('11', 'Prim_poly', '13'), Eq('MM', '3')), And(InRange('19', 'Prim_poly', '25'), Eq('MM', '4')), And(InRange('37', 'Prim_poly', '61'), Eq('MM', '5')), And(InRange('67', 'Prim_poly', '115'), Eq('MM', '6')), And(InRange('131', 'Prim_poly', '253'), Eq('MM', '7')), And(InRange('285', 'Prim_poly', '501'), Eq('MM', '8'))));\
Validate('In CCSDS mode [m] must be 8', Or(And(Neq('CCSDS', 'Conventional'), Eq('MM', '8')), Eq('CCSDS', 'Conventional')));\
Validate('In CCSDS mode [n] must be 255', Or(And(Neq('CCSDS', 'Conventional'), Eq('NN', '255')), Eq('CCSDS', 'Conventional')));\
Validate('In CCSDS 16 mode [t]=16, in CCSDS 8 mode [t]=8 ', Or(Eq('CCSDS', 'Conventional'), And(Eq('CCSDS', 'CCSDS 16'), Eq('TT', '16')), And(Eq('CCSDS', 'CCSDS 8'), Eq('TT', '8'))));\
Validate('Dual basis input converter can be used in CCSDS Mode only', Or(And(Eq('CCSDS', 'Conventional'), Eq('CCSDS_CONV', '0')), Neq('CCSDS', 'Conventional')));\
Validate('In CCSDS mode Primitive polynomial must be 391', Or(And(Neq('CCSDS', 'Conventional'), Eq('Prim_poly', '391')), Eq('CCSDS', 'Conventional')));\
Validate('Codeword must be longer than 2*[t]', Gt('NN', Mul('2', 'TT')));\
Validate('First Root value must be in the range from 0 to NN-1', Less('first_root', 'NN'));\
</actel-cc:script></spirit:vendorExtensions>
