  <spirit:vendorExtensions>
    <actel-cc:script>Enable('ECC', And(Neq('FAMILY', 'SmartFusion2'), Neq('FAMILY', 'IGLOO2')));\
Validate('LSRAM does not support Asynchronous Reads', Or(Neq('SEL_SRAM_TYPE', 'LSRAM'), Neq('PIPE', 'Asynchronous')));\
Validate('PolarFire USRAM does not support ECC', Or(Neq('SEL_SRAM_TYPE', 'USRAM'), Neq('FAMILY', 'PolarFire'), Eq('ECC', 'Disabled')));\
Validate('PolarFireSoC USRAM does not support ECC', Or(Neq('SEL_SRAM_TYPE', 'USRAM'), Neq('FAMILY', 'PolarFireSoC'), Eq('ECC', 'Disabled')));\
Validate('RTG4 USRAM does not support Asynchronous Read when ECC is configured to Pipelined ', Or(Neq('SEL_SRAM_TYPE', 'USRAM'), Neq('FAMILY', 'RTG4'), Neq('ECC', 'Pipelined'), Neq('PIPE', 'Asynchronous')));\
Validate('SmartFusion2 USRAM does not support ECC', Or(Neq('SEL_SRAM_TYPE', 'USRAM'), Neq('FAMILY', 'SmartFusion2'), Eq('ECC', 'Disabled')));\
Validate('IGLOO2 USRAM does not support ECC', Or(Neq('SEL_SRAM_TYPE', 'LSRAM'), Neq('FAMILY', 'IGLOO2'), Eq('ECC', 'Disabled')));\
Validate('SmartFusion2 LSRAM does not support Asynchronous Reads', Or(Neq('SEL_SRAM_TYPE', 'LSRAM'), Neq('FAMILY', 'SmartFusion2'), Neq('PIPE', 'Asynchronous')));\
Validate('IGLOO2 LSRAM does not support Asynchronous Reads', Or(Neq('SEL_SRAM_TYPE', 'LSRAM'), Neq('FAMILY', 'IGLOO2'), Neq('PIPE', 'Asynchronous')));\
</actel-cc:script></spirit:vendorExtensions>
