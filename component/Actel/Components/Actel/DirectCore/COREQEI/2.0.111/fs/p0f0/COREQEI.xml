        <spirit:size>16</spirit:size>
        <spirit:access>read-write</spirit:access>
        <spirit:resetValue>0x0000</spirit:resetValue>
        <spirit:description>Timer Load Register. Load the time interval value for velocity.</spirit:description>
      </spirit:register>
    </spirit:addressBlock>
    <spirit:bitsInLau>8</spirit:bitsInLau>
