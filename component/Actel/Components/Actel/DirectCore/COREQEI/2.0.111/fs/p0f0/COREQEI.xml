      <spirit:range>0x24</spirit:range>
      <spirit:width>32</spirit:width>
      <spirit:register>
        <spirit:name>QEI_CR</spirit:name>
        <spirit:addressOffset>0x00</spirit:addressOffset>
        <spirit:size>16</spirit:size>
        <spirit:access>read-write</spirit:access>
        <spirit:resetValue>0x0000</spirit:resetValue>
          <spirit:field>
            <spirit:name>FILTER_EN</spirit:name>
            <spirit:bitOffset>5</spirit:bitOffset>
            <spirit:bitWidth>1</spirit:bitWidth>
            <spirit:access>read-write</spirit:access>
            <spirit:description>Filter Enable/Disable bit
0 - Disable noise filter on input signals QEA, QEB and INDX
1 - Enable noise filter on input signals QEA, QEB and INDX </spirit:description>
          </spirit:field>
          <spirit:field>
            <spirit:name>QEIM[2:0]</spirit:name>
            <spirit:bitOffset>2</spirit:bitOffset>
            <spirit:bitWidth>3</spirit:bitWidth>
            <spirit:access>read-write</spirit:access>
            <spirit:description>QEI Mode bits
100-111 = reserved
011 =QEI enabled in 4x Update mode; position counter reset on period match
(POSCNT = MAXCNT)
010 =QEI enabled in 4x Update mode; INDX resets the position counter
001 =QEI enabled in 2x Update mode; position counter reset on period match
(POSCNT = MAXCNT)
000 =QEI enabled in 2x Update mode; INDX resets the position counter</spirit:description>
          </spirit:field>
          <spirit:field>
            <spirit:name>VPRR[1:0]</spirit:name>
            <spirit:bitOffset>0</spirit:bitOffset>
            <spirit:bitWidth>2</spirit:bitWidth>
            <spirit:access>read-write</spirit:access>
            <spirit:description>Velocity Pulse Reduction Ratio bit
11 =1:64
10 =1:16
01 =1:4
00=1:1</spirit:description>
          </spirit:field>
        <spirit:description>Control Register
</spirit:description>
      </spirit:register>
      <spirit:register>
        <spirit:name>QEI_VELOCITY</spirit:name>
        <spirit:addressOffset>0x04</spirit:addressOffset>
        <spirit:size>16</spirit:size>
        <spirit:access>read-only</spirit:access>
        <spirit:resetValue>0x0000</spirit:resetValue>
        <spirit:description>Velocity Register. This register contains the most recently measured velocity of the quadrature encoder. This value corresponds to the number of velocity pulses counted in the previous velocity timer period.</spirit:description>
      </spirit:register>
      <spirit:register>
        <spirit:name>QEI_POSITION</spirit:name>
        <spirit:addressOffset>0x08</spirit:addressOffset>
        <spirit:size>16</spirit:size>
        <spirit:access>read-only</spirit:access>
        <spirit:resetValue>0x0000</spirit:resetValue>
        <spirit:description>Position Register</spirit:description>
      </spirit:register>
      <spirit:register>
        <spirit:name>QEI_LIMIT</spirit:name>
        <spirit:addressOffset>0x0C</spirit:addressOffset>
        <spirit:size>16</spirit:size>
        <spirit:access>read-write</spirit:access>
        <spirit:resetValue>0x0000</spirit:resetValue>
        <spirit:description>Maximum Count  Limit Register</spirit:description>
      </spirit:register>
      <spirit:register>
        <spirit:name>QEI_IER</spirit:name>
        <spirit:addressOffset>0x10</spirit:addressOffset>
        <spirit:size>16</spirit:size>
        <spirit:access>read-write</spirit:access>
        <spirit:resetValue>0x0000</spirit:resetValue>
          <spirit:field>
            <spirit:name>DIR_INT</spirit:name>
            <spirit:bitOffset>2</spirit:bitOffset>
            <spirit:bitWidth>1</spirit:bitWidth>
            <spirit:access>read-write</spirit:access>
            <spirit:description>Enable/Disable Direction of Rotation Interrupt(DIR_INT)
1 = Enables the Direction of Rotation Interrupt(DIR_INT)
0 = Disables the Direction of Rotation Interrupt(DIR_INT)</spirit:description>
          </spirit:field>
          <spirit:field>
            <spirit:name>POS_INT</spirit:name>
            <spirit:bitOffset>1</spirit:bitOffset>
            <spirit:bitWidth>1</spirit:bitWidth>
            <spirit:access>read-write</spirit:access>
            <spirit:description>Enable/Disable Position Counter Interrupt(POS_INT)
1 = Enables Position Counter Interrupt(POS_INT)
0 = Disables Position Counter Interrupt(POS_INT)</spirit:description>
          </spirit:field>
          <spirit:field>
            <spirit:name>VEL_INT</spirit:name>
            <spirit:bitOffset>0</spirit:bitOffset>
            <spirit:bitWidth>1</spirit:bitWidth>
            <spirit:access>read-write</spirit:access>
            <spirit:description>Enable/Disable Velocity Register Update(VEL_INT)
1 = Enables Velocity Register Update(VEL_INT)
0 = Disables Velocity Register Update(VEL_INT)</spirit:description>
          </spirit:field>
        <spirit:description>Interrupt Enable Register</spirit:description>
      </spirit:register>
      <spirit:register>
        <spirit:name>QEI_SR</spirit:name>
        <spirit:addressOffset>0x14</spirit:addressOffset>
        <spirit:size>16</spirit:size>
        <spirit:access>read-write</spirit:access>
        <spirit:resetValue>0x0000</spirit:resetValue>
          <spirit:field>
            <spirit:name>ERROR</spirit:name>
            <spirit:bitOffset>4</spirit:bitOffset>
            <spirit:bitWidth>1</spirit:bitWidth>
            <spirit:access>read-only</spirit:access>
            <spirit:description>QEI error bit
1 = Position counter overflow or underflow
0 = No overflow or underflow</spirit:description>
          </spirit:field>
          <spirit:field>
            <spirit:name>UP/DOWN</spirit:name>
            <spirit:bitOffset>3</spirit:bitOffset>
            <spirit:bitWidth>1</spirit:bitWidth>
            <spirit:access>read-only</spirit:access>
            <spirit:description> Direction of Rotation Status bit
1 = Forward
0 = Reverse</spirit:description>
          </spirit:field>
          <spirit:field>
            <spirit:name>DIR_INT</spirit:name>
            <spirit:bitOffset>2</spirit:bitOffset>
            <spirit:bitWidth>1</spirit:bitWidth>
            <spirit:access>read-write</spirit:access>
            <spirit:description>Direction of Rotation Interrupt(DIR_INT)
1 = Direction of rotation has changed. Must be cleared by writing a &quot;1&quot;.
0 = Direction of rotation has not changed.</spirit:description>
          </spirit:field>
          <spirit:field>
            <spirit:name>POS_INT</spirit:name>
            <spirit:bitOffset>1</spirit:bitOffset>
            <spirit:bitWidth>1</spirit:bitWidth>
            <spirit:access>read-write</spirit:access>
            <spirit:description>Position Counter Interrupt(POS_INT)
1 = The QEI position counter has reached the MAXCNT value or the index pulse, INDX, has been detected. Depends on the QEI operating mode enabled. Must be cleared by writing a &quot;1&quot;.
0 = The QEI position counter has not reached the MAXCNT value or the index pulse has not been detected.</spirit:description>
          </spirit:field>
          <spirit:field>
            <spirit:name>VEL_INT</spirit:name>
            <spirit:bitOffset>0</spirit:bitOffset>
            <spirit:bitWidth>1</spirit:bitWidth>
            <spirit:access>read-write</spirit:access>
            <spirit:description>Velocity Register Update(VEL_INT)
1 = Velocity register updated. Must be cleared by writing a &quot;1&quot;. 
0 = Velocity register not updated.</spirit:description>
          </spirit:field>
        <spirit:description>Status Register</spirit:description>
      </spirit:register>
