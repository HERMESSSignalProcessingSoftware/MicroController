; ======================================== 
; MDDR                                
; ======================================== 



begin_param .
instance    MDDR .
param       MODE .
type        map  .
m_mapping     "MDDR_CONFIG:SDR_MODE"      .
  begin_map .
    map MODE_DDR 0 .
    map MODE_DDR_AXI64 0 .
    map MODE_DDR_AHB0 0 .
    map MODE_DDR_AHB1 0 .
    map MODE_DDR_AHB0_AHB1 0 .
    map MODE_SDR_AXI64 1 .
    map MODE_SDR_AHB 1 .
  end_map .
m_mapping     "MDDR_CONFIG:F_AXI_AHB_MODE"      .
  begin_map .
    map MODE_DDR 1 .
    map MODE_DDR_AXI64 1 .
    map MODE_DDR_AHB0 0 .
    map MODE_DDR_AHB1 0 .
    map MODE_DDR_AHB0_AHB1 0 .
    map MODE_SDR_AXI64 1 .
    map MODE_SDR_AHB 0 .
  end_map .
end_param .


begin_param .
instance    MDDR .
param       MEMORY_TYPE .
type        map  .
m_mapping     "MDDR_IO_CALIB:PCODE"      .
  begin_map .
    map LPDDR_NO_CALIBRATION 0x3d .
    map LPDDR_LVCMOS18_NO_CALIBRATION 0x3d .
    map LPDDR 0x0 .
    map LPDDR_LVCMOS18 0x0 .
    map DDR2 0x0 .
    map DDR2_FULL_STRENGTH 0x0 .
    map DDR3 0x0 .
    map DDR3_FULL_STRENGTH 0x0 .
  end_map .
m_mapping     "MDDR_IO_CALIB:NCODE"      .
  begin_map .
    map LPDDR_NO_CALIBRATION 0x3a .
    map LPDDR_LVCMOS18_NO_CALIBRATION 0x3a .
    map LPDDR 0x0 .
    map LPDDR_LVCMOS18 0x0 .
    map DDR2 0x0 .
    map DDR2_FULL_STRENGTH 0x0 .
    map DDR3 0x0 .
    map DDR3_FULL_STRENGTH 0x0 .
  end_map .
m_mapping     "MDDR_IO_CALIB:CALIB_TRIM"      .
  begin_map .
    map LPDDR_NO_CALIBRATION 1 .
    map LPDDR_LVCMOS18_NO_CALIBRATION 1 .
    map LPDDR 0 .
    map LPDDR_LVCMOS18 0 .
    map DDR2 0 .
    map DDR2_FULL_STRENGTH 0 .
    map DDR3 0 .
    map DDR3_FULL_STRENGTH 0 .
  end_map .
end_param .

; ======================================== 
;                                 
; ======================================== 


End.

Section PCBITS_REGISTERS_TO_SET_WHEN_INSTS_USED.



; ===============================================================================================
; Set the registers that need to be set to a different value when the inst is used.
; ===============================================================================================
CAN   "SOFTRESET:CAN_SOFTRESET"     "0" .



; ===============================================================================================
; Set the registers that need to be set to a different value when the inst is used.
; ===============================================================================================
;CC   "CC_CONFIG:CC_CACHE_ENB"     "0" .



; ===============================================================================================
; Set the registers that need to be set to a different value when the inst is used.
; ===============================================================================================
GPIO   "SOFTRESET:MSS_GPIO_SOFTRESET"     "0" .

; We should set the soft reset only when a gpio is used in the specific range we are setting.
; For now just set all of them if the GPIO is enabled.
GPIO   "SOFTRESET:MSS_GPOUT_7_0_SOFT_RESET"     "0" .
GPIO   "SOFTRESET:MSS_GPOUT_15_8_SOFT_RESET"    "0" .
GPIO   "SOFTRESET:MSS_GPOUT_23_16_SOFT_RESET"   "0" .
GPIO   "SOFTRESET:MSS_GPOUT_31_24_SOFT_RESET"   "0" .



; ===============================================================================================
; Set the registers that need to be set to a different value when the inst is used.
; ===============================================================================================
I2C_0   "SOFTRESET:I2C0_SOFTRESET"     "0" .
I2C_1   "SOFTRESET:I2C1_SOFTRESET"     "0" .



; ===============================================================================================
; Set the registers that need to be set to a different value when the inst is used.
; ===============================================================================================
MAC   "SOFTRESET:MAC_SOFTRESET"     "0" .



; ===============================================================================================
; Set the registers that need to be set to a different value when the inst is used.
; ===============================================================================================
MMUART_0   "SOFTRESET:MMUART0_SOFTRESET"     "0" .
MMUART_1   "SOFTRESET:MMUART1_SOFTRESET"     "0" .



; ===============================================================================================
; Set the registers that need to be set to a different value when the inst is used.
; ===============================================================================================
SPI_0   "SOFTRESET:G4SPI0_SOFTRESET"     "0" .
