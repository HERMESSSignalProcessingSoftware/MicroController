; ======================================== 
;                                 
; ======================================== 



; ======================================== 
; FIC32
; - There are two instances.
; ======================================== 

; =====================================================================================================
; USE_FAB_REGION_0, ... USE_FAB_REGION_5 they are mutualy exclusive between the FIC32_0 and FIC32_1
; 0 : Region is associated with FIC32_0
; 1 : Region is associated with FIC32_1
; For now we are going to read the data from just FIC32_0.
; If it's checked it means we are going to use FIC32_0, otherwise FIC32_1
; Later we need to see how we can implement that because we don't want to set the same bit twice
; for example.
; =====================================================================================================

; =============================================
; FIC32_0
; =============================================

begin_param .
instance FIC32_0 .
param    INTERFACE_TYPE .
type     map  .
m_mapping "FAB_IF:FAB0_AHB_MODE" .
  begin_map .
    map INTERFACE_AHB "'b1" .
    map INTERFACE_APB "'b0" .
  end_map .
end_param .

begin_param .
instance FIC32_0 .
param    USE_BYPASS_MODE .
type     bool  .
mapping  "FAB_IF:FAB0_AHB_BYPASS" .
end_param .

; ==============
; USE_FAB_REGION
; ==============
begin_param .
instance  FIC32_0 .
param     USE_FAB_REGION_0 .
type      bool .
m_mapping   "FAB_IF:SW_FIC_REG_SEL"  0 0 .
  begin_map .
    map  1   "'b0" .
    map  0   "'b1" .
  end_map .
end_param .

begin_param .
instance  FIC32_0 .
param     USE_FAB_REGION_1 .
type      bool .
m_mapping   "FAB_IF:SW_FIC_REG_SEL"  1 1 .
  begin_map .
    map  1   "'b0" .
    map  0   "'b1" .
  end_map .
end_param .

begin_param .
instance  FIC32_0 .
param     USE_FAB_REGION_2 .
type      bool .
m_mapping   "FAB_IF:SW_FIC_REG_SEL"  2 2 .
  begin_map .
    map  1   "'b0" .
    map  0   "'b1" .
  end_map .
end_param .

begin_param .
instance  FIC32_0 .
param     USE_FAB_REGION_3 .
type      bool .
m_mapping   "FAB_IF:SW_FIC_REG_SEL"  3 3 .
  begin_map .
    map  1   "'b0" .
    map  0   "'b1" .
  end_map .
end_param .

begin_param .
instance  FIC32_0 .
