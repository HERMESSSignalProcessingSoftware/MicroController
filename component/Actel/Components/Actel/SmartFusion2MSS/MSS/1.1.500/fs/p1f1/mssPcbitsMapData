m_mapping     "CC_CACHEREGION:CC_CACHE_REGION"      .
  begin_map .
    map 128MB_0001 "'b0001" .
    map 128MB_0010 "'b0010" .
    map 128MB_0100 "'b0100" .
    map 128MB_1000 "'b1000" .
    map 256MB_0011 "'b0011" .
    map 256MB_1100 "'b1100" .
    map 512MB_1111 "'b1111" .
  end_map .
end_param .

