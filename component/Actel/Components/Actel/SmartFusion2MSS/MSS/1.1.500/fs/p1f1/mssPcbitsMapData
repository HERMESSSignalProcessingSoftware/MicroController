mapping     "DDRB_BUF_TIMER:DDRB_TIMER"      .
end_param .


begin_param .
instance    DDRB .
param       DDRB_NB_ADDR .
type        hex  .
mapping     "DDRB_NB_ADR:DDRB_NB_ADDR"      .
end_param .


begin_param .
instance    DDRB .
param       DDRB_NB_SZ .
type        map  .
m_mapping     "DDRB_NB_SIZE:DDRB_NB_SZ"      .
  begin_map .
    map NONE "'b0000" .
    map 64KB "'b0001" .
    map 128KB "'b0010" .
    map 256KB "'b0011" .
    map 512KB "'b0100" .
    map 1MB "'b0101" .
    map 2MB "'b0110" .
    map 4MB "'b0111" .
    map 8MB "'b1000" .
    map 16MB "'b1001" .
    map 32MB "'b1010" .
    map 64MB "'b1011" .
    map 128MB "'b1100" .
    map 256MB "'b1101" .
    map 512MB "'b1110" .
    map 1GB "'b1111" .
  end_map .
end_param .


